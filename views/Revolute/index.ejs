        <div class="hstack" style="height: 70%; ">
          <video id="hidden" muted defaultmuted playsinline autoplay loop style="margin: 0px; width: 1px; height: 1px; padding: 0px; ">
            <source src="images/Revolute/empty.mp4" type="video/mp4" />
          </video>

          <video id="vid" class="video-bg" muted defaultmuted playsinline autoplay loop poster="images/Revolute/hero Medium.png" style="display: none; ">
            
            <source src="images/Revolute/Revolute2.mp4" type="video/mp4" />
            <!-- <source src="http://www.georgewpark.com/video/blurred-street.webm" type="video/webm" /> -->
          </video>

          <img id="image" class="video-bg" src="images/Revolute/hero Medium.png" alt="fallback" style="display: block; ">
        </div>


        <!-- <video class="video-bg" autoplay muted loop >
