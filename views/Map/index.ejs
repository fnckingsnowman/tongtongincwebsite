    .custom-btn {
    padding: 10px 25px;
    font-family: 'Lato', sans-serif;
    font-weight: 500;
    background: transparent;
    cursor: pointer;
    transition: all 0.3s ease;
    position: relative;
    display: inline-block;
    background-color: #1c2d40;
    border-radius: 10px;
    margin-left: 2%;
    box-shadow:
            -10px -10px 30px 1px #ffffff11,
            -6px -6px 10px 2px #ffffff11,
            10px 10px 30px 1px #000000b9,
            6px 6px 10px 2px #000000b9;

    
    }
   
    .slideleft {
    border: none;
    color: #8b8b8b;
    z-index: 1;
    }

    .slideleft:after {
    position: absolute;
    content: "";
    width: 0;
    height: 100%;
    top: 0;
    right: 0;
    z-index: -1;
    box-shadow: inset -4px -4px 8px rgba(255, 255, 255, 0.135),
            inset 8px 8px 16px rgba(0, 0, 0, 0.605);
    transition: all 0.3s ease;
    border-radius: 10px;
    
    }

    .slideleft:hover {
    color: #efeac8;
   
    }

    .slideleft:hover:after {
    left: 0;
    width: 100%;
    }

    .slideleft:active {
    top: 2px;
    
    }




 </style>
    
