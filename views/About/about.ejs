    <script>




        document.addEventListener('DOMContentLoaded', getCommits);

        async function getCommits() {
            const repoName = 'username/repo'; // Set your GitHub repository here

            try {
                const response = await fetch(`https://api.github.com/repos/tongtongwang86/tongtongincwebsite/commits`);
                const commits = await response.json();

                displayCommits(commits);
            } catch (error) {
                console.error('Error fetching data:', error);
                alert('Error fetching data. Please check the repository name and try again.');
            }
        }

        function displayCommits(commits) {
            const commitList = document.getElementById('commitList');
            commitList.innerHTML = ''; // Clear previous results

            if (commits.length === 0) {
                commitList.innerHTML = '<p>No commits found for this repository.</p>';
                return;
            }

            const ul = document.createElement('ul');

            commits.forEach(commit => {
                const li = document.createElement('li');
                const commitDate = new Date(commit.commit.author.date);
                const formattedDate = formatDate(commitDate);

                li.textContent = `${commit.commit.author.name}: ${commit.commit.message} - ${formattedDate}`;
                ul.appendChild(li);
            });

            commitList.appendChild(ul);
        }

        function formatDate(date) {
            const options = { month: 'short', day: 'numeric', hour: 'numeric', hour12: true };
            return new Intl.DateTimeFormat('en-US', options).format(date);
        }


function millisecondsToYears(milliseconds) {
  const millisecondsInYear = 31557600000; // 365.25 days in milliseconds
  return milliseconds / millisecondsInYear;
}

function yearsToDays(years) {
  const daysInYear = 365;
  let totalDays = 0;

  for (let year = 1; year <= years; year++) {
    const currentYearDays = (year % 4 === 0 && (year % 100 !== 0 || year % 400 === 0)) ? daysInYear + 1 : daysInYear;
    totalDays += currentYearDays;
  }

  return totalDays;
}


