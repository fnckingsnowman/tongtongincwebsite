.conic .squircle {
  position: absolute;
  width: 100%;
  height: 100%;
  z-index: -1;

  --bg: #1c1d2c;
}

.conic:active {
  box-shadow: 0 0 3rem #f5dfa2;
  /* filter: drop-shadow(0px 0px 5px rgba(255, 255, 255, 0.2))
    drop-shadow(0px 0px 10px rgba(0, 0, 0, 0.605)); */
  transition: 0.01s ease;
  top: 5px;
}
.filledbar {
  /*         position: relative;
        top: 50px; */
  /* z-index: 3; */
  width: 50px;
  height: 50px;
  transform: scale(2.3);
  /*         border-radius: 10%; */
  /* background: rgb(0,154,217); */
  /* background: linear-gradient(90deg, rgba(0,154,217,1) 0%, rgba(217,147,0,1) 65%, rgba(255,186,0,1) 100%); */

  transition: 0.6s ease-out;
}

.conic:hover .filledbar {
  /* width: 120px;
  height: 100px; */
  transform: scale(3.2);
  transition: 0.4s ease-out;
}

.bar {
  display: grid;
  margin-bottom: 20%;
  place-items: center;
}

.conic:not(:first-child) {
  margin-left: -40px;
}

.conic:hover {
  /*   color: #efeac8;
  transform: scale(0.98); */

  transform: scale(1.05) translateY(-20px);
  transition: 0.4s ease-out;

  /*    filter: 
