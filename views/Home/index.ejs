.conic {
  display: flex;
  text-decoration: none;
  font-family: "Lato", sans-serif;
  font-weight: 500;
  position: relative;
  width: 195px;
  height: 280px;
  box-sizing: border-box;
  display: grid;
  place-items: center;
  /*   margin: 0 8px; */
  cursor: pointer;
  left: 0px;
  transition: all 0.3s ease;
  color: #b8b8b8;
  transform: scale(1);
  /*   top: 0px; */
  border-radius: 30px;
  box-shadow: 0 0 3rem #000;

  /*   filter: drop-shadow(4px 4px 5px rgba(50, 50,50, .6)); */
}

